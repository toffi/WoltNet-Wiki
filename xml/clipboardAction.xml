<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/clipboardAction.xsd">
	<import>
		<!-- article -->
		<action name="assignLabel">
			<actionclassname><![CDATA[wiki\system\clipboard\action\ArticleClipboardAction]]></actionclassname>
			<showorder>1</showorder>
			<pages>
				<page><![CDATA[wiki\page\CategoryPage]]></page>
				<page><![CDATA[wiki\page\ArticlePage]]></page>
			</pages>
		</action>
		<action name="enable">
			<actionclassname><![CDATA[wiki\system\clipboard\action\ArticleClipboardAction]]></actionclassname>
			<showorder>2</showorder>
			<pages>
				<page><![CDATA[wiki\page\CategoryPage]]></page>
				<page><![CDATA[wiki\page\ArticlePage]]></page>
			</pages>
		</action>
		<action name="trash">
			<actionclassname><![CDATA[wiki\system\clipboard\action\ArticleClipboardAction]]></actionclassname>
			<showorder>3</showorder>
			<pages>
				<page><![CDATA[wiki\page\CategoryPage]]></page>
				<page><![CDATA[wiki\page\ArticlePage]]></page>
			</pages>
		</action>
		<action name="delete">
			<actionclassname><![CDATA[wiki\system\clipboard\action\ArticleClipboardAction]]></actionclassname>
			<showorder>4</showorder>
			<pages>
				<page><![CDATA[wiki\page\CategoryPage]]></page>
				<page><![CDATA[wiki\page\ArticlePage]]></page>
			</pages>
		</action>
		<action name="restore">
			<actionclassname><![CDATA[wiki\system\clipboard\action\ArticleClipboardAction]]></actionclassname>
			<showorder>5</showorder>
			<pages>
				<page><![CDATA[wiki\page\CategoryPage]]></page>
				<page><![CDATA[wiki\page\ArticlePage]]></page>
			</pages>
		</action>
		<!-- /article -->
	</import>
</data>